<!DOCTYPE html>
<html>
<head>
	<title>List of relays</title>
</head>
<body>
	<h1>List of Relays for current user</h1>
<ul>
	<% relays.forEach(function(relay){ %>
		<li>Number of channels: <%= relay.no_channels%></li>
		<li>Subnet id: <%= relay.subnetId%></li>
		<li>Relay id: <%= relay.deviceId%></li>
		<li>User id: <%= relay.UserId%></li>
		<a href="/admin/relay/<%= relay.id%>/channels">See Channels</a><br>
		<a href="/admin/relay/<%= relay.id%>/channel/create">Create Channel</a>
		<form action="/admin/relay/delete/" method="post">
			<input type="hidden" name="relay_id" value="<%=relay.id%>">
			<input type="hidden" name="UserId" value="<%=UserId%>">
			<input type="submit" value="delete">
		</form>
		<br><br>


	<%})%>
</ul>


</body>
</html>